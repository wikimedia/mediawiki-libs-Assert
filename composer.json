{
	"name": "wikimedia/assert",
	"description": "Provides runtime assertions",
	"keywords": ["PHP", "QA", "assert", "assertions", "precondition", "postcondition"],
	"homepage": "https://github.com/wikimedia/Assert",
	"license": "MIT",
	"authors": [
		{
			"name": "Daniel Kinzler"
		},
		{
			"name": "Thiemo Kreuz"
		}
	],
	"autoload": {
		"psr-4": {
			"Wikimedia\\Assert\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Wikimedia\\Assert\\Test\\": "tests/phpunit/"
		}
	},
	"require": {
		"php": ">=5.5.9"
	},
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "0.8.0",
		"phpunit/phpunit": "^4.8"
	},
	"scripts": {
		"test": [
			"phpunit",
			"phpcs -p -s"
		],
		"fix": "phpcbf",
		"cover": "phpunit --coverage-html coverage/"
	}
}
